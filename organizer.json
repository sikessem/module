{
    "plateform": {
        "sapi": "*",
        "zend": "*",
        "php": ">=8.0",
        "lib": {
            "Phar": {
                "version": "*",
                "options": {
                    "phar.readonly": true
                }
            },
            "Tokenizer": {
                "version": "*"
            }
        },
        "bin": [
            "composer",
            "git"
        ],
        "env": {},
        "os": {
            "name": "*",
            "family": "*"
        }
    },
    "system": {
        "bundle": "organizer/organizer",
        "namespace": "Organizer",
        "version": "0.0.0",
        "root": "src/core",
        "main": "organizer.php",
        "modules": {
            "types": {
                "interface": {
                    "(.*)_Interface": "\\1.interface",
                    ".*_Interface": "\\0",
                    "(.*)Interface": "\\1.interface",
                    ".*Interface": "\\0",
                    "(.*)_Interface": "\\1",
                    "(.*)Interface": "\\1"
                },
                "abstract": {
                    "Abstract_(.*)": "\\1.abstract",
                    "Abstract_.*": "\\0",
                    "Abstract(.*)": "\\1.abstract",
                    "Abstract.*": "\\0",
                    "Abstract_(.*)": "\\1",
                    "Abstract(.*)": "\\1"
                },
                "final": {
                    "Final_(.*)": "\\1.final",
                    "Final_.*": "\\0",
                    "Final(.*)": "\\1.final",
                    "Final.*": "\\0",
                    "Final_(.*)": "\\1",
                    "Final(.*)": "\\1"
                },
                "trait": {
                    "(.*)_Trait": "\\1.trait",
                    ".*_Trait": "\\0",
                    "(.*)Trait": "\\1.trait",
                    ".*Trait": "\\0",
                    "(.*)_Trait": "\\1",
                    "(.*)Trait": "\\1"
                },
                "class": {
                    ".*": "\\0.class",
                    ".*": "\\0"
                }
            },
            "folders": [
                "src",
                "lib",
                "pkg",
                "mod",
                "obj",
                "libs",
                "pkgs",
                "mods",
                "objs",
                "tools",
                "components",
                "libraries",
                "library",
                "packages",
                "package",
                "sources",
                "source",
                "modules",
                "module",
                "objects",
                "object",
                "php_tools",
                "php_components",
                "php_libraries",
                "php_library",
                "php_packages",
                "php_package",
                "php_sources",
                "php_source",
                "php_modules",
                "php_module",
                "php_objects",
                "php_object"
            ],
            "extensions": [
                ".php",
                ".inc.php",
                ".lib.php",
                ".pkg.php",
                ".mod.php",
                ".obj.php",
                ".app.php",
                ".inc.lib.php",
                ".inc.pkg.php",
                ".inc.mod.php",
                ".inc.obj.php",
                ".inc.app.php",
                ".inc.phar",
                ".lib.phar",
                ".pkg.phar",
                ".mod.phar",
                ".obj.phar",
                ".app.phar",
                ".inc.lib.phar",
                ".inc.pkg.phar",
                ".inc.mod.phar",
                ".inc.obj.phar",
                ".inc.app.phar",
                ".phar",
                ".inc",
                ""
            ]
        }
    }
}